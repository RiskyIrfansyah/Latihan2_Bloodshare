<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.latihan2_bloodshare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Latihan2_Blodeshare">
        <activity android:name="com.example.latihan2_bloodshare.Home"></activity>
        <activity android:name="com.example.latihan2_bloodshare.Verify" />
        <activity android:name="com.example.latihan2_bloodshare.Almost" />
        <activity android:name="com.example.latihan2_bloodshare.Register" />
        <activity android:name="com.example.latihan2_bloodshare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>